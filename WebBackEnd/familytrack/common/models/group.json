{
  "name": "Group",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Name": {
      "type": "string",
      "required": true,
      "default": "family 1"
    },
    "Description": {
      "type": "string",
      "required": true,
      "default": "familia marquez"
    }
  },
  "validations": [],
  "relations": {
    "group_members": {
      "type": "hasMany",
      "model": "group_member",
      "foreignKey": "GroupId"
    },
    "members": {
      "type": "hasMany",
      "model": "Group_member",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
